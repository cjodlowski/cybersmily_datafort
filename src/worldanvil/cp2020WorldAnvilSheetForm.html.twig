<div class="container-fluid bg-white black">
  <br>
  <div class="row">
    <div class="col-12 col-lg-6">
      <div class="row">
        <div class="col-2 bg-black border border-dark">
          NAME
        </div>
        <div class="col-9 border border-dark">
          <input value="{{ variables.name is defined ? variables.name }}" required="required"
            class="d-inline-block w-100" id="name" name="name"
            placeholder="The name of the Cyberpunk 2020 Character Sheet" type="text" />
        </div>
      </div>
      <div class="row mt-1">
        <div class="col-3 bg-black border border-dark">
          HANDLE
        </div>
        <div class="col-8 border border-dark">
          <input value="{{ variables.handle is defined ? variables.handle }}" class="d-inline-block w-100" id="handle"
            name="handle" placeholder="Handle/nickname/streetname" type="text" />
        </div>
      </div>
      <div class="row mt-1">
        <div class="col-2 bg-black border border-dark">
          ROLE
        </div>
        <div class="col-4 border border-dark">
          <input value="{{ variables.primary_role is defined ? variables.primary_role}}" class="d-inline-block w-100"
            id="primary_role" name="primary_role" type="text" />
          </span>
        </div>
        <div class="col-2"></div>
        <div class="col-2 bg-black">REP</div>
        <div class="col-1 border border-dark">
          <input value="{{ variables.rep is defined ? variables.rep : 1 }}" class="d-inline-block w-100 no-spinners" id="rep" name="rep"
            type="number" min="0" />
        </div>
      </div>
      <div class="row mt-1">
        <div class="col-3 bg-black border border-dark small">
          SECONDARY ROLE
        </div>
        <div class="col-4 border border-dark">
          <input value="{{ variables.secondaryrole01 is defined ? variables.secondaryrole01 }}" class="w-100"
            id="secondaryrole01" name="secondaryrole01" type="text" />
        </div>
      </div>
      <div class="row mt-1">
        <div class="col-2 bg-black">STATS</div>
        <div class="col-1 border border-dark">
          <input value="{{ variables.stat_pts is defined ? variables.stat_pts : 0 }}" class="d-inline-block w-100 text-center no-spinners"
            id="stat_pts" name="stat_pts" type="number" min="0" />
        </div>
        <div class="col-5"></div>
        <div class="col-2 px-0 text-right">Humanity:</div>
