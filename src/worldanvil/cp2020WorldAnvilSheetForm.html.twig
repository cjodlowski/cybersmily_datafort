    <input type="number" value="{{ev}}" title="name of armor"
      class="w-100 d-inline-block m-0 p-0 no-spinners text-center"
      id="layer{{id}}_EV" name="layer{{id}}_EV">
  </div>
  <div class="col-1 border border-dark p-0">
    <select value="{{type}}" title="type of armor"
      class="w-100 d-inline-block m-0 p-0"
