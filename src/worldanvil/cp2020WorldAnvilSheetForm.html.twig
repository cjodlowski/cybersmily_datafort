      </div>
      <div class="row black font-weight-bold mt-1">
        {{ sheet.genStat('COOL', 'cool', variables.cool, variables.coolcurr) }}
        {{ sheet.genStat('ATTR', 'attr', variables.attr, variables.attrcurr) }}
        {{ sheet.genStat('LUCK', 'luck', variables.luck, variables.luckcurr) }}
