        <div class="col-1 pl-0 pr-1 border border-dark">
          <input value="{{ variables.leap is defined ? variables.leap : variable.macurr * 0.75}}" class="d-inline-block w-100 text-center" id="leap"
            name="leap" type="number" min="0" />
        </div>
      </div>
      <div class="row black font-weight-bold mt-1">
        {{ sheet.genStat('MA', 'ma', variables.ma, variables.macurr) }}
        {{ sheet.genStat('BODY', 'body', variables.body, variables.bodycurr) }}
        {{ sheet.genStat('EMP', 'emp', variables.emp, variables.empcurr) }}
