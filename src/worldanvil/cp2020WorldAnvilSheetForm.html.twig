{% macro genSibling(id, name, gender, feeling, age) %}
  <div class="row">
    <div class="col-5 border border-dark pl-1 pr-0 ">
      <input value="{{ name }}" class="d-inline-block w-100"
        id="sibling{{id}}name" name="sibling{{id}}name" type="text"/>
    </div>
    <div class="col-2 border border-dark p-0 ">
      <select value="{{gender}}" class="d-inline-block w-100"
        id="sibling{{id}}gender" name="sibling{{id}}gender">
        <option value="Sib" {% if gender is defined and gender == "Sib"  %}selected="selected" {% endif %}  >Sib</option>
        <option value="Brother" {% if gender is defined and gender == "Brother"  %}selected="selected" {% endif %}  >Brother</option>
        <option value="Sister" {% if gender is defined and gender == "Sister"  %}selected="selected" {% endif %}  >Sister</option>
      </select>
    </div>
    <div class="col-2 border border-dark p-0 ">
      <select value="{{age}}" class="d-inline-block w-100"
        id="sibling{{id}}age" name="sibling{{id}}age">
        <option value="Older" {% if age is defined and age == "Older"  %}selected="selected" {% endif %}  >Older</option>
        <option value="Younger" {% if age is defined and age == "Younger"  %}selected="selected" {% endif %}  >Younger</option>
        <option value="Twin" {% if age is defined and age == "Twin"  %}selected="selected" {% endif %}  >Twin</option>
      </select>
    </div>
    <div class="col-3 border border-dark p-0 ">
      <select   class="form-control"  id="sibling{{id}}feelings" name="sibling{{id}}feelings">
          <option value="Neutral" {% if feeling is defined and feeling == "Neutral"  %}selected="selected" {% endif %}  >Neutral</option>
          <option value="Hero Worship" {% if feeling is defined and feeling == "Hero Worship"  %}selected="selected" {% endif %}  >Hero Worship</option>
          <option value="Likes" {% if feeling is defined and feeling == "Likes"  %}selected="selected" {% endif %}  >Likes</option>
          <option value="Dislikes" {% if feeling is defined and feeling == "Dislikes"  %}selected="selected" {% endif %}  >Dislikes</option>
          <option value="Hates" {% if feeling is defined and feeling == "Hates"  %}selected="selected" {% endif %}  >Hates</option>
      </select>
    </div>
  </div>
{% endmacro genSibling %}
