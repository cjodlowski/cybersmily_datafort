{% import _self as sheet %}
{# ******************************************************************************************************************************** #}
{% macro genLifeEventsSection(variables) %}
  <div class="row border border-dark p-0">
     <div class="col font-weight-bold bg-black pl-1">LIFE EVENTS</div>
  </div>
  <div class="row border border-dark p-0">
     <div class="col-1 pl-1 border border-dark">Year</div>
     <div class="col-11 pl-1 border border-dark">Event</div>
  </div>
   {% for i in 1..20 %}
     {% set id= i  %}
     {% if id < 10 %}
       {% set id = '0' ~id %}
     {% endif  %}
     {{ sheet.genLifeEvent(id, variables['lifeEventYear'~id], variables['lifeEventDetail'~id]|trim)}}
   {% endfor %}{% endmacro genLifeEventsSection %}
{# ******************************************************************************************************************************** #}
{% macro genLifeEvent(id, year, event) %}
  <div class="row border-dark">
    <div class="col-1 border border-dark">
      <input value="{{ year }}" class="d-inline-block w-100 text-center no-spinners"
        id="lifeEventYear{{id}}" name="lifeEventYear{{id}}" type="number" min="0" />
    </div>
    <div class="col-11 border border-dark">
      <textarea rows="3" cols="40" id="lifeEventDetail{{id}}" name="lifeEventDetail{{id}}"
        class="d-inline-block w-100 p-0 m-0 cp2020-lifeevent-details">
      {{event}}
      </textarea>
    </div>
  </div>
{% endmacro genLifeEvent %}
{# ******************************************************************************************************************************** #}
{% macro genSkillSection(variables, statName, statSkillNames, length, statValue) %}
<div class="col-12">
  <div class="row border border-dark pb-1 pt-1">
    <div class="col-1 font-weight-bold">{{statName}}</div>
    <div class="col-11">
      <div class="card-columns">
        <div class="card">
          <div class="row small">
            <div class="col-9 pl-3">Name</div>
            <div class="col-2 p-0">Rank</div>
            <div class="col-1 p-0">IP</div>
          </div>
        </div>
        <div class="d-none d-sm-inline-block card">
          <div class="row small">
            <div class="col-9 pl-3">Name</div>
            <div class="col-2 p-0">Rank</div>
            <div class="col-1 p-0">IP</div>
          </div>
        </div>
        <div class="d-none d-sm-inline-block card">
          <div class="row small">
            <div class="col-9 pl-3">Name</div>
            <div class="col-2 p-0">Rank</div>
            <div class="col-1 p-0">IP</div>
          </div>
        </div>
      </div>
      <div class="card-columns">
        {% for skillName in statSkillNames %}
          {% set id= loop.index  %}
          {% if id < 10 %}
            {% set id = '0' ~id %}
          {% endif  %}
          {{ sheet.genSkill(id, statName, skillName , variables[statName~'_SkillValue_'~id], variables[statName~'_SkillIP_'~id], variables[statName~'_SkillChipped_'~id], statValue)}}
        {% endfor %}
      </div>
    </div>
  </div>
</div>{% endmacro genSkillSection %}
{# ******************************************************************************************************************************** #}
{% macro genSkill(id, title, skillName, skillValue, ip, isChipped, statValue) %}
  <div class="card">
    <div class="row">
      <div class="col p-0 pl-1">
        <input value="{{ isChipped }}" title="Is a Chipped Skill"
          class="d-inline-block align-middle" id="{{title}}_skillchipped_{{id}}" name="{{title}}_skillchipped_{{id}}" type="checkbox"/>
          {{ skillName }}
          <span class="float-right">
          {{' [' ~ (statValue + skillValue) ~ ']'}}
          </span>
      </div>
      <div class="col-2 border border-dark p-0">
        <input value="{{ skillValue }}" title="Skill Value"
          class="d-inline-block w-100 p-0 m-0 no-spinners   text-center" id="{{title}}_SkillValue_{{id}}" name="{{title}}_SkillValue_{{id}}" type="number" min="0" max="15" />
      </div>
      <div class="col-1 border border-dark p-0">
        <input value="{{ ip }}" title="Skill IP"
          class="d-inline-block w-100 p-0 m-0 no-spinners   text-center" id="{{title}}_SkillIP_{{id}}" name="{{title}}_SkillIP_{{id}}" type="number" min="0" max="1000" />
      </div>
    </div>
  </div>
{% endmacro genSkill %}
{# ******************************************************************************************************************************** #}
{% macro genSkillName(id, title, skillName, skillValue, ip, isChipped, statValue) %}
  <div class="card">
    <div class="row">
      <div class="col p-0">
        <input value="{{ isChipped }}" title="Is a Chipped Skill"
          class="d-inline-block align-middle" id="{{title}}skillchipped{{id}}" name="{{title}}skillchipped{{id}}" type="checkbox"/>
          {{title}}:
          <input
            value="{{ skillName }}"
            title="Skill Name"
            type="text"
            class="d-inline-block border border-dark w-50 p-0 ml-1 mr-1"
            id="{{title}}skillname{{id}}"
            name="{{title}}skillname{{id}}"/>
          <span class="float-right">
          {{' [' ~ (statValue + skillValue) ~ ']'}}
          </span>
      </div>
      <div class="col-2 border border-dark p-0">
        <input value="{{ skillValue }}" title="Skill Value"
          class="d-inline-block w-100 p-0 m-0 no-spinners text-center" id="{{title}}skillvalue{{id}}" name="{{title}}skillvalue{{id}}" type="number" min="0" max="15" />
      </div>
      <div class="col-1 border border-dark p-0">
        <input value="{{ ip }}" title="Skill IP"
          class="d-inline-block w-100 p-0 m-0 no-spinners   text-center" id="{{title}}skillip{{id}}" name="{{title}}skillip{{id}}" type="number" min="0" max="1000" />
      </div>
    </div>
  </div>
{% endmacro genSkillName %}
{# ******************************************************************************************************************************** #}
{% macro genSpecialAbility(title, skillName, skillValue, ip) %}
  <div class="col-12 col-md-6 col-xl-4 align-middle">
    <div class="row">
      <div class="col-9">
          <input
            value="{{ skillName }}"
            title="Skill Name"
            type="text"
            class="d-inline-block border border-dark w-100 p-1"
            id="{{title}}"
            name="{{title}}"/>
      </div>
      <div class="col-2 border border-dark p-0">
        <input value="{{ skillValue }}" title="Skill Value"
          class="d-inline-block w-100 p-1 m-0 no-spinners   text-center" id="{{title}}_value" name="{{title}}_value" type="number" min="0" max="15" />
      </div>
      <div class="col-1 border border-dark p-0">
        <input value="{{ ip }}" title="Skill IP"
          class="d-inline-block w-100 p-1 m-0 no-spinners   text-center" id="{{title}}_ip" name="{{title}}_ip" type="number" min="0" max="1000" />
      </div>
    </div>
  </div>
{% endmacro genSpecialAbility %}
{# ******************************************************************************************************************************** #}
{% macro genStat(name, stat, baseValue, currValue) %}
  {% set bv = 0 %}
  {% if baseValue is defined and baseValue >= 0 %}
    {% set bv = baseValue %}
  {% endif %}
  {% set cv = 0 %}
  {% if currValue is defined and currValue >= 0 %}
    {% set cv = currValue %}
  {% endif %}
  <div class="col-1 pl-0 pr-1 text-right">{{name}}</div>
  <div class="col-1 pl-0 pr-0 text-right border border-dark">
    <input value="{{ bv }}" title="Base {{ name }} Value"
    class="d-inline-block w-100 text-center" id="{{stat}}Base" name="{{stat}}Base" type="number" min="0" />
  </div>
  <div class="col-1 pl-0 pr-0 text-right border border-dark">
    <input value="{{ cv }}" title="Current {{ name }} Value"
    class="d-inline-block w-100 text-center" id="{{stat}}Curr" name="{{stat}}Curr" type="number" min="0" />
  </div>
{% endmacro genStat %}
{# ******************************************************************************************************************************** #}
{% macro genGear(id, value) %}
    <div class="col-6 col-md-4 col-xl-3 border border-dark pl-1 pr-0 ">
      <input value="{{ value }}" class="d-inline-block w-100"
        id="gearName_{{id}}" name="gearName_{{id}}" type="text" row="1" />
    </div>
{% endmacro genGear %}
{# ******************************************************************************************************************************** #}
{% macro genWeapon(id, name, note, type, acc, conc, dmg, shots, rof, rng, rel, mag ) %}
  <div class="row p-0 small">
    <div class="col-3 border border-dark p-0">
      <input type="text" id="weaponName_{{id}}" name="weaponName_{{id}}"
        placeholder="name of weapon"
        value="{{name}}" class="d-inline-block w-100 p-0 m-0">
    </div>
    <div class="col-1 border border-dark p-0">
      <select value="{{type}}" class="d-inline-block w-100 p-0 m-0"
        id="weaponType_{{id}}" name="weaponType_{{id}}" title="Weapon type">
        <option value="P">P</option>
        <option value="RIF">RIF</option>
        <option value="SMG">SMG</option>
        <option value="SHT">SHT</option>
        <option value="MEL">MEL</option>
        <option value="EX">EX</option>
        <option value="HVY">HVY</option>
      </select>
    </div>
    <div class="col-1 border border-dark p-0">
      <input type="number" min="-15" max="15" class="d-inline-block w-100 p-0 m-0 no-spinners"
        placeholder="wa"
        value="{{acc}}" id="weaponAcc_{{id}}" name="weaponAcc_{{id}}" title="Weapon Accuracy">
    </div>
    <div class="col-1 border border-dark p-0">
      <select value="{{conc}}" class="d-inline-block w-100 p-0 m-0"
        id="weaponConc_{{id}}" name="weaponConc_{{id}}" title="Weapon concealment">
        <option value="P">P</option>
        <option value="J">J</option>
        <option value="L">L</option>
        <option value="N">N</option>
      </select>
    </div>
    <div class="col-2 border border-dark p-0">
      <input type="number" min="0" max="1000" class="d-inline-block w-100 p-0 m-0 no-spinners"
        placeholder="ex. 2D6+1"
        value="{{dmg}}" id="weaponDmg_{{id}}" name="weaponDmg_{{id}}" title="Weapon Damage">
    </div>
    <div class="col-1 border border-dark p-0">
      <input type="number" min="0" max="1000" class="d-inline-block w-100 p-0 m-0 no-spinners"
        placeholder="# shots"
        value="{{shots}}" id="weaponShots_{{id}}" name="weaponShots_{{id}}" title="Weapon #Shots">
    </div>
    <div class="col-1 border border-dark p-0">
      <input type="number" min="0" max="1000" class="d-inline-block w-100 p-0 m-0 no-spinners"
        placeholder="rof"
        value="{{rof}}" id="weaponRof_{{id}}" name="weaponRof_{{id}}" title="Weapon RoF"></div>
    <div class="col-1 border border-dark p-0">
      <input type="number" min="0" max="1000" class="d-inline-block w-100 p-0 m-0 no-spinners"
        placeholder="range"
        value="{{rng}}" id="weaponRng_{{id}}" name="weaponRng_{{id}}" title="Weapon Range in meters">
    </div>
    <div class="col-1 border border-dark p-0">
      <select value="{{rel}}" class="d-inline-block w-100 p-0 m-0"
        id="weaponConc_{{id}}" name="weaponConc_{{id}}" title="Weapon concealment">
        <option value="ST">ST</option>
        <option value="UR">UR</option>
        <option value="VR">VR</option>
      </select>
    </div>
  </div>
  <div class="row small border border-dark ">
    <div class="col-1">Notes:</div>
    <div class="col-11">
      <textarea rows="3" cols="40" id="weaponNote_{{id}}" name="weaponNote_{{id}}" class="d-inline-block w-100 p-0 m-0 cp2020-weapon-notes">{{note}}</textarea>
    </div>
  </div>
{% endmacro genWeapon %}
{# ******************************************************************************************************************************** #}
{% macro genCyber(id, value, hl) %}
<div class="card">
  <div class="row">
    <div class="col-10 border border-dark">
      <input value="{{ value }}" class="d-inline-block w-100"
        id="cybername{{id}}" name="cybername{{id}}" type="text" row="1" />
    </div>
    <div class="col-2 border border-dark">
      <input value="{{ value }}" class="d-inline-block w-100 no-spinners"
        id="cyberhl{{id}}" name="cyberhl{{id}}" type="number" min="0" />
    </div>
  </div>
</div>{% endmacro genCyber %}
{# ******************************************************************************************************************************** #}
{% macro genWoundSection(name, stunMod, wounds) %}
  <div class="row">
    <div class="col-12 pr-0 pl-0 text-center">{{ name }}</div>
  </div>
  <div class="row">
    <div class="col-12 form-check-inline text-center mr-0 ml-0 pl-0">
      <input type="checkbox" class="d-inline-block mr-0 ml-0 text-center" {% if wounds >= 1 %}checked{% endif %}>
      <input type="checkbox" class="d-inline-block mr-0 ml-0 text-center" {% if wounds >= 2 %}checked{% endif %}>
      <input type="checkbox" class="d-inline-block mr-0 ml-0 text-center" {% if wounds >= 3 %}checked{% endif %}>
      <input type="checkbox" class="d-inline-block mr-0 ml-0 text-center" {% if wounds >= 4 %}checked{% endif %}>
    </div>
  </div>
  <div class="row bg-black">
    <div class="col-12 pr-0 pl-0 text-center">Stun={{ stunMod }}</div>
  </div>
{% endmacro genWoundSection %}
{# ******************************************************************************************************************************** #}
